    "@tanstack/vue-virtual": "^3.13.12",
    "@types/reconnectingwebsocket": "^1.0.10",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.13.2",
    "countup.js": "^2.9.0",
    "dayjs": "^1.11.19",
    "dompurify": "^3.3.0",
    "echarts": "^6.0.0",
    "ipaddr.js": "^2.2.0",
    "lodash": "^4.17.21",
    "misans": "^4.1.0",
    "p-limit": "^7.2.0",
